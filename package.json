{
  "name": "animelist",
  "version": "1.1.0",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "bundle": "browserify --ignore-missing --no-builtins --no-commondir --insert-global-vars=\"global\" -t vueify -e public/js/main.js -o public/js/build.js --no-commondir",
    "build": "electron-packager . AnimeList --platform=win32 --arch=x64 --electron-version=1.4.12 --out=dist --ignore=dist --asar=true --prune --overwrite --icon=icons/AL.ico",
    "build-32": "electron-packager . AnimeList --platform=win32 --arch=ia32 --electron-version=1.4.12 --out=dist --asar=true --ignore=dist --prune --overwrite --icon=icons/AL.ico",
    "build-linux": "electron-packager . AnimeList --platform=linux --arch=x64 --electron-version=1.4.12 --out=dist --asar=true --ignore=dist --prune --overwrite"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "nedb": "^1.8.0",
    "request": "^2.82.0",
    "vue-scrollto": "^2.7.8",
    "vue-shortkey": "^3.0.2",
    "winston": "^2.3.1",
    "winston-electron": "^0.1.0"
  },
  "devDependencies": {
    "browserify": "^14.4.0",
    "electron": "^1.6.11",
    "vueify": "^9.4.1"
  }
}
